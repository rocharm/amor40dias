<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="net.ibmemorial.ummes.model.ControleEmail" table="TCONTROLEEMAIL">
        <id name="codigo" type="java.lang.Integer">
            <column name="ID" precision="10" scale="0" />
            <generator class="native" />
        </id>
        <property name="codigoInscrito" type="java.lang.Integer" column="CODIGOINSCRITO" precision="10" scale="0" />
        <property name="tipoEmail" type="java.lang.Integer" column="TIPO" precision="2" scale="0" />
    </class>
</hibernate-mapping>