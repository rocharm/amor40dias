<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="net.ibmemorial.ummes.model.InscritoHorario" table="THORARIO_FACILHOSP">
        <composite-id name="id" class="net.ibmemorial.ummes.model.InscritoHorarioId">
            <key-property name="codigoInscrito" type="java.lang.Integer">
                <column name="IDFACILHOSP" precision="10" scale="0" />
            </key-property>
            <key-property name="codigoHorario" type="java.lang.Integer">
                <column name="IDHORARIO" precision="1" scale="0" />
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>
